            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".postlogin_entrenador"
            android:label="@string/title_activity_postlogin_entrenador"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".editar_perfil_usuario"

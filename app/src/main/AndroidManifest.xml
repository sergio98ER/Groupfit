    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".usuarioejercicios_rutina"></activity>
        <activity android:name=".cambiarentrenousuario" />
        <activity android:name=".informacionrutina" />
        <activity android:name=".editarperfilusuario_desde_entrenador" />
        <activity android:name=".perfilusuario_grupo" />
        <activity android:name=".listausuariosgrupo" />
        <activity android:name=".fragment_info_grupo" />
        <activity android:name=".editarperfilentrenador" />
        <activity android:name=".vistaprevia_aboutpage" />
        <activity
            android:name=".postinicioentrenador"
